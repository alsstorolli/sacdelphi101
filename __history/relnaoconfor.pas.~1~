      end;
      if trim(PConsumo.mesano11)<>'' then begin
        FRel.AddCel(floattostr(PConsumo.qtde11));
        FRel.AddCel(floattostr(PConsumo.valor11));
        perc:=0;
        if PConsumo.valor11>0 then
          perc:=(PConsumo.qtde11/PConsumo.valor11)*100;

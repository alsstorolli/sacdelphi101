      FRel.AddCol( 65,3,'N','+' ,f_cr              ,PConsumo.mesano10       ,''         ,'',false);
      FRel.AddCol( 65,3,'N','+' ,f_cr              ,'Previsto'          ,''         ,'',false);
      FRel.AddCol( 50,3,'N',''  ,f_cr              ,'%'          ,''         ,'',false);

procedure TFConfigura.EdPEDVENDA01FValidate(Sender: TObject);
begin
   if EdPedvenda01f.ascurrency<EdPedvenda01.ascurrency then
     EdPedvenda01f.invalid('Valor tem que ser maior que o mínimo');
end;

procedure TFConfigura.EdPEDVENDA02FValidate(Sender: TObject);
begin
   if EdPedvenda02f.ascurrency<EdPedvenda02.ascurrency then
     EdPedvenda02f.invalid('Valor tem que ser maior que o inicial');

end;

procedure TFConfigura.bprocuraClick(Sender: TObject);
begin
  if (opendialog1.execute) and ( trim(opendialog1.FileName)<>'' ) then
    EdLocalpea.Text:=ExtractFilePath(Opendialog1.FileName)
end;

procedure TFConfigura.balteracontadorClick(Sender: TObject);
var serie,nomecontador:string;
begin
  if Ednumeronfs.IsEmpty then exit;
  if not Input('Informe a série','Série',serie,1,true ) then exit;
  if trim(serie)='' then exit;
  if not confirma('Alterar o contador de nota de saida da unidade '+Global.CodigoUnidade) then exit;
  nomecontador:='NFSAIDA'+Global.CodigoUnidade+serie;
  try
    FGeral.AlteraContador(nomecontador,EdNumeronfs.AsInteger);
    Aviso('Contador alterado para '+EdNumeronfs.Text);
  except
    Avisoerro('Problemas na alteração da numeração da nota');
  end;

end;

procedure TFConfigura.EdtamanholetraValidate(Sender: TObject);
begin
  if (Edtamanholetra.asinteger<0) or (Edtamanholetra.asinteger>12) then
    EdTamanholetra.invalid('Tamanho da letra varia entre 0 e 12'); 
end;

procedure TFConfigura.pcontabilDblClick(Sender: TObject);
begin

  FGeral.AtualizaExcel;

end;

// 03.07.12
procedure TFConfigura.SetaAtalhos(Ed:TSqled);
////////////////////////////////////////////////
var p,r,i:integer;
begin
  Ed.Items.Clear;
  {
  for p:=0 to Application.ComponentCount-1 do begin
     if Application.Components[p] is TForm then begin
       if Ansipos( TForm(Application.Components[p]).Caption,'FGeral/FInit' ) = 0 then
         Ed.Items.Add(  TForm(Application.Components[p]).Caption );
     end;
  end;
  }
  for p:=0 to FMain.Menu.Items.Count-1 do begin
    for r:=0 to FMain.Menu.Items[p].Count-1 do begin
      for i:=0 to FMain.Menu.Items[p].Items[r].Count-1 do begin
        Ed.Items.Add(  FMain.Menu.Items[p].Items[r].Items[i].Name );
      end;
    end;
  end;
// 25.04.18 - para colocar itens de sub-menus
  Ed.Items.Add( 'RelaoPendentes2' );

//  Ed.Items.Sort;
end;

procedure TFConfigura.bcor1Click(Sender: TObject);
begin
   if Cd.Execute then EdCoreditsnaoenable.text:=ColorToString( Cd.color );
end;

procedure TFConfigura.EdFaixafrcValidate(Sender: TObject);
begin
   if EdFaixafrc.ascurrency<Edfaixairc.ascurrency then EdFaixafrc.invalid('Faixa final deve ser maior que a inicial');
end;

procedure TFConfigura.bgravaimpClick(Sender: TObject);
//////////////////////////////////////////////////////////////
begin
//   if not Edimpnfe.isempty then CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPNFE',EdImpnfe.text);
//   if not Edimpnfce.isempty then CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPNFCE',EdImpnfce.text);
// 05.06.19
   CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPNFE',EdImpnfe.text);
   CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPNFCE',EdImpnfce.text);
   if not Edimproment.isempty then CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPROMENT',Edimproment.text);
   if not EdIMPETQENT.isempty then CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPETQENT',Edimpetqent.text);
// 07.04.17
   if not EdIMPboleto.isempty then CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPBOLETO',Edimpboleto.text);
// 08.04.18
   if not EdIMPvalidas.isempty then CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPVALIDA',Edimpvalidas.text);
   if not EdIMPETQENTada.isempty then CriaArquivoINI(Global.NomeSistema,'Impressoras','IMPETQENTADA',EdimpetqentADA.text);
end;

